{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Schema for InternalCtrlr",
  "type": "object",
  "properties": {
    "ChargePointId": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "type": "string"
    },
    "CentralSystemURI": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "type": "string"
    },
    "ChargeBoxSerialNumber": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 25,
      "type": "string"
    },
    "ChargePointModel": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 20,
      "type": "string"
    },
    "ChargePointSerialNumber": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 25,
      "type": "string"
    },
    "ChargePointVendor": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 20,
      "type": "string"
    },
    "FirmwareVersion": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 50,
      "type": "string"
    },
    "ICCID": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 20,
      "type": "string"
    },
    "IMSI": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 20,
      "type": "string"
    },
    "MeterSerialNumber": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 25,
      "type": "string"
    },
    "MeterType": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "string"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minLength": 1,
      "maxLength": 25,
      "type": "string"
    },
    "SupportedCiphers12": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "MemberList"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "ECDHE-ECDSA-AES128-GCM-SHA256,ECDHE-ECDSA-AES256,GCM-SHA384,AES128-GCM-SHA256,AES256-GCM-SHA384,TLS_AES_256_GCM_SHA384,TLS_AES_128_GCM_SHA256",
      "minLength": 0,
      "type": "string"
    },
    "SupportedCiphers13": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "MemberList"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "TLS_AES_256_GCM_SHA384,TLS_AES_128_GCM_SHA256",
      "minLength": 0,
      "type": "string"
    },
    "WebsocketReconnectInterval": {
      "characteristics": {
        "minLimit": 0,
        "supportsMonitoring": true,
        "dataType": "integer"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minimum": 0,
      "default": "10",
      "type": "integer"
    },
    "AuthorizeConnectorZeroOnConnectorOne": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "boolean"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "1",
      "type": "boolean"
    },
    "LogMessages": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "boolean"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "1",
      "type": "boolean"
    },
    "LogMessagesFormat": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "MemberList"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "description": "Supported log formats are console, log, html and console_detailed",
      "default": "log,html",
      "type": "string"
    },
    "SupportedChargingProfilePurposeTypes": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "MemberList"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "ChargePointMaxProfile,TxDefaultProfile,TxProfile",
      "type": "string"
    },
    "MaxCompositeScheduleDuration": {
      "characteristics": {
        "supportsMonitoring": true,
        "dataType": "integer"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "default": "31536000",
      "type": "integer"
    },
    "NumberOfConnectors": {
      "characteristics": {
        "minLimit": 1,
        "maxLimit": 128,
        "supportsMonitoring": true,
        "dataType": "integer"
      },
      "attributes": [
        {
          "type": "Actual",
          "mutability": "ReadOnly"
        }
      ],
      "minimum": 1,
      "maximum": 128,
      "default": "1",
      "type": "integer"
    }
  },
  "required": [
    "CentralSystemURI",
    "ChargeBoxSerialNumber",
    "ChargePointId",
    "ChargePointModel",
    "ChargePointVendor",
    "FirmwareVersion",
    "NumberOfConnectors",
    "SupportedCiphers12",
    "SupportedCiphers13",
    "WebsocketReconnectInterval"
  ]
}
